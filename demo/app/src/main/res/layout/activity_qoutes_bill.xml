<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".qoutes_bill">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="0dp"
        app:srcCompat="@drawable/page_q" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="406dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="98dp"
        android:layout_marginEnd="0dp"
        android:fontFamily="@font/alike"
        android:text="“We all need people who will give us feedback. That’s how we improve.”"
        android:textColor="@color/black"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/text66"
        android:layout_width="406dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="3dp"
        android:fontFamily="@font/alike"
        android:text="“Patience is a key element of success.”"
        android:textColor="@color/black"
        android:textSize="20dp" />


    <TextView
        android:id="@+id/text54"
        android:layout_width="406dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text66"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="1dp"
        android:fontFamily="@font/alike"
        android:text="“It’s fine to celebrate success, but it is more important to heed the lessons of failure.”"
        android:textColor="@color/black"
        android:textSize="20dp" />


    <TextView
        android:id="@+id/text34"
        android:layout_width="406dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text54"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="1dp"
        android:fontFamily="@font/alike"
        android:text="“Dont compare yourself with anyone in this world, if you do so ... you are insulting yourself.”"
        android:textColor="@color/black"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/text14"
        android:layout_width="406dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text34"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="-3dp"
        android:layout_marginBottom="110dp"
        android:fontFamily="@font/alike"
        android:text="“Your most unhappy customers are your greatest source of learning.”"
        android:textColor="@color/black"
        android:textSize="20dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="520dp"
        android:layout_marginEnd="325dp"
        android:layout_marginBottom="1dp"
        app:srcCompat="@drawable/page_q" />


</RelativeLayout>